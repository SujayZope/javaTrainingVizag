package com.infinite.Collections;

import java.awt.List;
import java.util.ArrayList;

public class ListDemo1 {
	
	public static void main(String[] args) {
		List names=new ArrayList();
		names.add("Pavan");
		names.add("Chandu");
		names.add("Lata");
		names.add("Azhar");
		names.add("Chetan");
		
		
	}

}
